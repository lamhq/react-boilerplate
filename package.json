    "react-hook-form": "^7.45.1",
    "react-router-dom": "^6.14.0",
    "reflect-metadata": "^0.1.13",
    "tailwind-merge": "^1.14.0",
    "tsyringe": "^4.8.0",
    "yup": "^1.2.0"
