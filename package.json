{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamhq/react-boilerplate"
  },
  "author": "lamhq",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3001",
